{
  "name": "cospik",
  "version": "1.0.0",
  "description": "An app that allows a university admin to pick a course for students",
  "main": "index.js",
  "scripts": {
    "start": "yarn run check-types && yarn run migrate && node build/index.js",
    "check-types": "tsc",
    "dev": "nodemon src/index.ts",
    "dev-migrate": "NODE_ENV=test ts-node ./src/db/db.ts",
    "migrate": "node ./build/db/db.js",
    "pretest": "yarn run dev-migrate && yarn run dev-seeder",
    "dev-seeder": "NODE_ENV=test ts-node ./src/seeder.ts",
    "test": "jest --testTimeout=10000 --runInBand --forceExit --coverage",
    "codeclimate-report": "codeclimate-test-reporter < ./coverage/lcov.info"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "expressjs"
  ],
  "author": "Ayodeji",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.9",
    "@types/express-graphql": "^0.9.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.10",
    "@types/yup": "^0.29.11",
    "codeclimate-test-reporter": "^0.5.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.12.0",
    "morgan": "^1.10.0",
    "tsutils": "^3.21.0",
    "yup": "^0.32.9"
  }
}
